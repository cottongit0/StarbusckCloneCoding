size: 0.75em;\r\n            padding: 11px 16px;\r\n            display: block;\r\n            color: $sub-grey;\r\n            &:hover {\r\n              color: $real-black;\r\n            }\r\n          }\r\n          &:first-child::before {\r\n            display: none;\r\n          }\r\n          &::before {\r\n            position: absolute;\r\n            content: \"\";\r\n            width: 1px;\r\n            height: 0.75em;\r\n            background-color: $bright-grey;\r\n            top: 0;\r\n            bottom: 0;\r\n            margin: auto;\r\n          }\r\n        }\r\n      }\r\n      .search {\r\n        position: relative;\r\n        height: 34px;\r\n        input {\r\n          width: 36px;\r\n          height: 34px;\r\n          padding: 4px 10px;\r\n          border: 1px solid #ccc;\r\n          box-sizing: border-box;\r\n          border-radius: 5px;\r\n          outline: none;\r\n          background-color: $real-white;\r\n          color: $sub-grey;\r\n          font-size: 0.75em;\r\n          transition: width 0.4s;\r\n          &:focus {\r\n            width: 190px;\r\n            border-color: $main-green;\r\n          }\r\n        }\r\n        .material-icons {\r\n          height: 24px;\r\n          position: absolute;\r\n          top: 0;\r\n          bottom: 0;\r\n          right: 5px;\r\n          margin: auto;\r\n          transition: 0.4s;\r\n        }\r\n        &.focused {\r\n          .material-icons {\r\n            opacity: 0;\r\n          }\r\n        }\r\n      }\r\n    }\r\n\r\n    // main-menu\r\n    .main-menu {\r\n      position: absolute;\r\n      bottom: 0;\r\n      right: 0;\r\n      z-index: 1;\r\n      display: flex;\r\n      .item {\r\n        .item__name {\r\n          text-transform: uppercase;\r\n          padding: 10px 20px 34px 20px;\r\n          font-family: Arial, Helvetica, sans-serif;\r\n          font-size: 0.813em;\r\n        }\r\n        .item__contents {\r\n          width: 100%;\r\n          position: fixed;\r\n          left: 0;\r\n          display: none;\r\n          .contents__menu {\r\n            background-color: red;\r\n            height: 200px;\r\n          }\r\n          .contents__texture {\r\n            background-color: orange;\r\n            height: 100px;\r\n          }\r\n        }\r\n        &:hover {\r\n          .item__name {\r\n            background-color: $darker-grey;\r\n            color: $main-green;\r\n            border-radius: 6px 6px 0 0;\r\n          }\r\n          .item__contents {\r\n            display: block;\r\n            .contents__menu {\r\n            }\r\n            .contents__texture {\r\n            }\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n\r\n  // badge\r\n  .badges {\r\n    position: absolute;\r\n    top: 132px;\r\n    right: 12px;\r\n    .badge {\r\n      border-radius: 10px;\r\n      overflow: hidden;\r\n      margin-bottom: 12px;\r\n      box-shadow: 4px 4px 10px rgba($color: $real-black, $alpha: 0.15);\r\n      cursor: pointer;\r\n    }\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"492915c952ed3c155a00aac1af25631e","cacheData":{"env":{}}}
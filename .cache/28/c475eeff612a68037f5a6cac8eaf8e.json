r-grey: #2c2a29;\r\n\r\n$real-black: #000;\r\n$real-white: #fff;\r\n\r\n// COMMON\r\nbody {\r\n  color: $main-font;\r\n  font-size: 16px;\r\n  font-weight: 400;\r\n  line-height: 1.4;\r\n  font-family: \"Nanum Gothic\", sans-serif;\r\n\r\n  height: 3000px;\r\n\r\n  img {\r\n    display: block;\r\n  }\r\n  a {\r\n    text-decoration: none;\r\n  }\r\n  .inner {\r\n    position: relative;\r\n    width: 1100px;\r\n    margin: 0 auto;\r\n  }\r\n}\r\n\r\n// HEADER\r\nheader {\r\n  position: fixed;\r\n  background-color: $main-white;\r\n  border-bottom: 1px solid #c8c8c8;\r\n  top: 0;\r\n  width: 100%;\r\n\r\n  > .inner {\r\n    height: 120px;\r\n\r\n    // logo\r\n    .logo {\r\n      height: 75px;\r\n      position: absolute;\r\n      top: 0;\r\n      bottom: 0;\r\n      margin: auto;\r\n    }\r\n\r\n    // sub-menu\r\n    .sub-menu {\r\n      position: absolute;\r\n      top: 10px;\r\n      right: 0;\r\n      display: flex;\r\n      .menu {\r\n        font-family: Arial, Helvetica, sans-serif;\r\n        display: flex;\r\n        li {\r\n          position: relative;\r\n          a {\r\n            font-size: 0.75em;\r\n            padding: 11px 16px;\r\n            display: block;\r\n            color: $sub-grey;\r\n            &:hover {\r\n              color: $real-black;\r\n            }\r\n          }\r\n          &:first-child::before {\r\n            display: none;\r\n          }\r\n          &::before {\r\n            position: absolute;\r\n            content: \"\";\r\n            width: 1px;\r\n            height: 0.75em;\r\n            background-color: $bright-grey;\r\n            top: 0;\r\n            bottom: 0;\r\n            margin: auto;\r\n          }\r\n        }\r\n      }\r\n      .search {\r\n        position: relative;\r\n        height: 34px;\r\n        input {\r\n          width: 36px;\r\n          height: 34px;\r\n          padding: 4px 10px;\r\n          border: 1px solid #ccc;\r\n          box-sizing: border-box;\r\n          border-radius: 5px;\r\n          outline: none;\r\n          background-color: $real-white;\r\n          color: $sub-grey;\r\n          font-size: 0.75em;\r\n          transition: width 0.4s;\r\n          &:focus {\r\n            width: 190px;\r\n            border-color: $main-green;\r\n          }\r\n        }\r\n        .material-icons {\r\n          height: 24px;\r\n          position: absolute;\r\n          top: 0;\r\n          bottom: 0;\r\n          right: 5px;\r\n          margin: auto;\r\n          transition: 0.4s;\r\n        }\r\n        &.focused {\r\n          .material-icons {\r\n            opacity: 0;\r\n          }\r\n        }\r\n      }\r\n    }\r\n\r\n    // main-menu\r\n    .main-menu {\r\n      display: flex;\r\n      position: absolute;\r\n      bottom: 0;\r\n      right: 0;\r\n      z-index: 1;\r\n      .item {\r\n        .item__name {\r\n          padding: 10px 20px 34px 20px;\r\n          font: {\r\n            family: Arial, sans-serif;\r\n            size: 13px;\r\n          }\r\n          &:hover {\r\n            cursor: pointer;\r\n            background-color: $darker-grey;\r\n            color: $main-green;\r\n            border-radius: 6px 6px 0 0;\r\n          }\r\n        }\r\n        .item__contents {\r\n          width: 100%;\r\n          position: fixed;\r\n          left: 0;\r\n          display: none;\r\n          .contents__menu {\r\n            background-color: $darker-grey;\r\n            > ul {\r\n              display: flex;\r\n              padding: 20px 0;\r\n              > li {\r\n                width: 220px;\r\n                h4 {\r\n                  padding: 3px 0 12px 0;\r\n                  font-size: 14px;\r\n                  color: $real-white;\r\n                }\r\n                ul {\r\n                  li {\r\n                    padding: 5px 0;\r\n                    font-size: 0.75em;\r\n                    color: $main-grey;\r\n                    cursor: pointer;\r\n                    &:hover {\r\n                      color: $main-green;\r\n                    }\r\n                  }\r\n                }\r\n              }\r\n            }\r\n          }\r\n          .contents__texture {\r\n            background-image: url(\"../images/main_menu_pattern.jpg\");\r\n            padding: 26px 0;\r\n            font-size: 0.75em;\r\n            h4 {\r\n              color: $main-grey;\r\n              font-weight: 700;\r\n            }\r\n            p {\r\n              color: $main-green;\r\n              margin: 4px 0 14px;\r\n            }\r\n          }\r\n        }\r\n        &:hover {\r\n          .item__contents {\r\n            display: block;\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n\r\n  // badge\r\n  .badges {\r\n    position: absolute;\r\n    top: 132px;\r\n    right: 12px;\r\n    .badge {\r\n      border-radius: 10px;\r\n      overflow: hidden;\r\n      margin-bottom: 12px;\r\n      box-shadow: 4px 4px 10px rgba($color: $real-black, $alpha: 0.15);\r\n      cursor: pointer;\r\n    }\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"c678c850102a25f6a235f567b60ef6e6","cacheData":{"env":{}}}